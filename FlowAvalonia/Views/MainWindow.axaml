    <Design.DataContext>
        <!-- This only sets the DataContext for the previewer in an IDE,
             to set the actual DataContext for runtime, set the DataContext property in code (look at App.axaml.cs) -->
        <vm:MainWindowWindowViewModel />
    </Design.DataContext>

    <Window.KeyBindings>
        <KeyBinding
            Command="{Binding  MainViewModel.MoveResultUp}"
            Gesture="Up" />
        <KeyBinding
            Command="{Binding MainViewModel.MoveResultDown}"
            Gesture="Down" />
        <KeyBinding
            Command="{Binding ResultsBoxViewModel.ToContextMenu}"
            Gesture="SHIFT+Enter" />
        <KeyBinding
            Command="{Binding ResultsBoxViewModel.Escape}"
            Gesture="Escape" />
    </Window.KeyBindings>

    <Grid DataContext="{Binding MainViewModel}">
        <Border>
            <StackPanel>
                <Border>
                    <TextBox
                        x:Name="QueryTextBox"
                        Text="{Binding QueryText, Mode=TwoWay}"
                        IsVisible="True"
                        Height="50"
                        Margin="10"
                        FontSize="24">
                        <TextBox.Styles>
                            <Style Selector="TextBox">
                                <Setter Property="BorderBrush" Value="Transparent"/>
                                <Setter Property="BorderThickness" Value="0" />
                            </Style>
                            <Style Selector="TextBox:pressed, TextBox:focus, TextBox:watermark">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="BorderBrush" Value="Transparent" />
                            </Style>
                        </TextBox.Styles>
                        <TextBox.ContextMenu>
                            <ContextMenu MinWidth="160">
                                <MenuItem Header="Cut" Icon="{StaticResource CutRegular}">
                                </MenuItem>
                                <MenuItem Header="Copy" Icon="{StaticResource DocumentCopyRegular}">
                                </MenuItem>
                                <MenuItem Header="Paste" Icon="{StaticResource ClipboardPasteRegular}">
                                </MenuItem>
                                <Separator
                                    Margin="0"
                                    Padding="0,4,0,4"
                                    Background="White" />
                                <MenuItem Header="Settings" Icon="{StaticResource LauncherSettingsRegular}">
                                </MenuItem>
                                <MenuItem Header="Close" Icon="{StaticResource PaneCloseRegular}">
                                </MenuItem>
                            </ContextMenu>
                        </TextBox.ContextMenu>
                    </TextBox>
                </Border>
                <Border>
                    <view:ResultItemBox />
                </Border>
            </StackPanel>
        </Border>
    </Grid>
</Window>